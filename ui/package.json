{
  "name": "@compository/grapes",
  "version": "0.0.1",
  "description": "TODO: change description",
  "author": "guillem.cordoba@gmail.com",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm:start-ui\" \"npm:start-holochain\"",
    "start-ui": "CONDUCTOR_URL=ws://localhost:8888 concurrently -k --names rollup -c rollup.config.js,dev-server \"npm run build-watch\" \"web-dev-server --config demo/web-dev-server.config.mjs\"",
    "start-holochain": "cd .. && RUST_LOG=warn hc s generate -r=8888 zome/happ",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "build-watch": "rollup -c rollup.config.js --watch",
    "test": "rollup -c rollup.config.js && web-test-runner --coverage --puppeteer",
    "test-debug": "rollup -c rollup.config.js && DEBUG=true web-test-runner --coverage --puppeteer",
    "test-watch": "web-test-runner --watch --puppeteer",
    "e2e": "CONDUCTOR_URL=ws://localhost:8888 concurrently -k -s first \"npm:test\" \"npm:start-holochain\"",
    "analyze": "wca analyze src --format json --outFile custom-elements.json",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore",
    "storybook": "concurrently -k --names rollup -c rollup.config.js,storybook \"npm run build-watch\" \"start-storybook --node-resolve --watch --open\"",
    "build-storybook": "npm run analyze && build-storybook",
    "publish-storybook": "npm run lint && npm run analyze && npm run build&& npm run build-storybook && gh-pages -d storybook-static",
    "publish-npm": "npm run lint && npm run build && cp ../README.md dist/  && cp ../LICENSE dist/ && cp package.json dist/ && cd dist/ && npm publish --access public && rimraf dist/",
    "publish-to-branch": "npm run lint && npm run build && gh-pages -d . -b ui-build"
  },
  "dependencies": {
    "@compository/lib": "git+https://github.com/compository/lib.git#build",
    "@holochain-open-dev/profiles": "git+https://github.com/holochain-open-dev/profiles.git#ui-build",
    "@holochain/conductor-api": "git+https://github.com/holochain/holochain-conductor-api.git",
    "@open-wc/scoped-elements": "^1.3.3",
    "grapesjs": "^0.16.41",
    "grapesjs-blocks-basic": "^0.1.8",
    "grapesjs-preset-webpage": "^0.1.11",
    "i": "^0.3.6",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "npm": "^7.6.0",
    "scoped-material-components": "^0.1.3"
  },
  "devDependencies": {
    "@holochain-open-dev/holochain-run-dna": "^0.2.11",
    "@open-wc/demoing-storybook": "^2.0.0",
    "@open-wc/eslint-config": "^4.2.0",
    "@open-wc/testing": "^2.0.0",
    "@open-wc/testing-karma": "^3.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@web/dev-server": "0.0.13",
    "@web/dev-server-rollup": "^0.2.9",
    "@web/test-runner": "^0.7.41",
    "@web/test-runner-puppeteer": "^0.6.4",
    "buffer": "^5.6.0",
    "concurrently": "^5.1.0",
    "deepmerge": "^3.2.0",
    "es-dev-server": "^1.23.0",
    "es-dev-server-rollup": "0.0.8",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^8.1.0",
    "gh-pages": "^3.1.0",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "postcss": "^8.1.9",
    "postcss-import": "12.0.1",
    "postcss-url": "^10.1.1",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-postcss-lit": "^1.0.1",
    "tslib": "^1.11.0",
    "typescript": "~3.9.0",
    "web-component-analyzer": "^1.1.6"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
